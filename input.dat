import pybind_plugin as pb # register options here

molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5
}

set {
  basis sto-3g
}

e,wfn = energy('scf',return_wfn=True)

set PYBIND_PLUGIN {
  PYBIND_PLUGIN_PRINT 2
}

pb.myplugin(wfn,psi4.core.get_options())

options.set_current_module('PYBIND_PLUGIN')
print "PYBIND_PLUGIN_PRINT = ", options.get_int("PYBIND_PLUGIN_PRINT")
